<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:orientation="vertical"
    android:background="#f6f6f6"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        app:cardBackgroundColor="@color/white"
        app:cardElevation="2dp">
        <include layout="@layout/toolbar_location"/>
    </androidx.cardview.widget.CardView>
    <LinearLayout
        android:orientation="vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="23dp"
            android:layout_marginBottom="16dp">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/ic_past_time"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Plumber"
                android:fontFamily="@font/roboto_regular"/>
        </LinearLayout>
        <View
            android:background="#e1e1e1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="23dp"
            android:layout_marginBottom="16dp">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/ic_past_time"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Lights"
                android:fontFamily="@font/roboto_regular"/>
        </LinearLayout>
        <View
            android:background="#e1e1e1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="23dp"
            android:layout_marginBottom="16dp">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/ic_past_time"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Photographer"
                android:fontFamily="@font/roboto_regular"/>
        </LinearLayout>
    </LinearLayout>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="11.7dp"
        android:layout_marginBottom="11dp"
        app:cardBackgroundColor="@color/white">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="15dp">
            <TextView
                android:textSize="11.7sp"
                android:textColor="#8f909e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:text="POPUlAR CITIES"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:text="New Dallas"
                android:fontFamily="@font/roboto_regular"/>
            <View
                android:background="#e1e1e1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="14sp"
                android:layout_marginRight="12dp"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:text="South Carli"
                android:fontFamily="@font/roboto_regular"/>
            <View
                android:background="#e1e1e1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="14sp"
                android:layout_marginRight="12dp"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:text="West Jacky"
                android:fontFamily="@font/roboto_regular"/>
            <View
                android:background="#e1e1e1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="14sp"
                android:layout_marginRight="12dp"/>
            <TextView
                android:textSize="15sp"
                android:textColor="@color/walkthrough_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:text="Franzborough"
                android:fontFamily="@font/roboto_regular"/>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>
